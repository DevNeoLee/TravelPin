<h3> from new</h3>
<h2> signing up </h2>

<%= form_with model: @user, url: users_path, local: true do |f| %>
  <% if @user.errors.any? %>
    <% @user.errors.full_messages.each do |error| %>
        <div><%= error %></div>
    <% end %>
  <% end %>

  email: <%= f.text_field :email %><br>
  password: <%= f.password_field :password %><br>
  password_confirm: <%= f.password_field :password_confirmation %>
  <%= f.submit "submit" %>
<% end %>
<hr>